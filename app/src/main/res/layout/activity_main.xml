<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="devinstech.space.ticatytac.MainActivity"
    android:background="@drawable/paper">

    <ImageButton
        android:id="@+id/bottomRight"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginLeft="-24dp"
        android:layout_marginTop="18dp"
        android:onClick="playBoxClicked"
        app:layout_constraintLeft_toLeftOf="@+id/middleRight"
        app:layout_constraintTop_toBottomOf="@+id/middleRight"
        app:srcCompat="@drawable/transsquare" />

    <ImageButton
        android:id="@+id/topLeft"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="0dp"
        android:onClick="playBoxClicked"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/topMiddle"
        app:srcCompat="@drawable/transsquare" />

    <ImageButton
        android:id="@+id/topMiddle"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="0dp"
        android:onClick="playBoxClicked"
        app:layout_constraintLeft_toRightOf="@+id/topLeft"
        app:layout_constraintTop_toTopOf="@+id/topRight"
        app:srcCompat="@drawable/transsquare" />

    <ImageButton
        android:id="@+id/middleLeft"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginLeft="-28dp"
        android:layout_marginTop="0dp"
        android:onClick="playBoxClicked"
        app:layout_constraintLeft_toLeftOf="@+id/topLeft"
        app:layout_constraintTop_toTopOf="@+id/middleMiddle"
        app:srcCompat="@drawable/transsquare" />

    <ImageButton
        android:id="@+id/middleMiddle"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginLeft="-41dp"
        android:layout_marginTop="-7dp"
        android:onClick="playBoxClicked"
        app:layout_constraintLeft_toLeftOf="@+id/topMiddle"
        app:layout_constraintTop_toTopOf="@+id/middleRight"
        app:srcCompat="@drawable/transsquare" />

    <ImageButton
        android:id="@+id/topRight"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="41dp"
        android:onClick="playBoxClicked"
        app:layout_constraintLeft_toRightOf="@+id/topMiddle"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/transsquare" />

    <ImageButton
        android:id="@+id/middleRight"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginLeft="-28dp"
        android:layout_marginTop="25dp"
        android:onClick="playBoxClicked"
        app:layout_constraintLeft_toLeftOf="@+id/topRight"
        app:layout_constraintTop_toBottomOf="@+id/topRight"
        app:srcCompat="@drawable/transsquare" />

    <ImageButton
        android:id="@+id/bottomLeft"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginLeft="-31dp"
        android:layout_marginTop="0dp"
        android:onClick="playBoxClicked"
        app:layout_constraintLeft_toLeftOf="@+id/middleLeft"
        app:layout_constraintTop_toTopOf="@+id/bottomMiddle"
        app:srcCompat="@drawable/transsquare" />

    <ImageButton
        android:id="@+id/bottomMiddle"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginLeft="-25dp"
        android:layout_marginTop="-8dp"
        android:onClick="playBoxClicked"
        app:layout_constraintLeft_toLeftOf="@+id/middleMiddle"
        app:layout_constraintTop_toTopOf="@+id/bottomRight"
        app:srcCompat="@drawable/transsquare" />

    <TextView
        android:id="@+id/curPlayerName"
        android:layout_width="313dp"
        android:layout_height="33dp"
        android:text="Player_Name"
        android:textAppearance="@android:style/TextAppearance.Material.Body2"
        android:textSize="24sp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toBottomOf="@+id/bottomMiddle"
        app:layout_constraintHorizontal_bias="0.854" />

</android.support.constraint.ConstraintLayout>
